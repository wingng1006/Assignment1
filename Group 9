    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.model_selection import RandomizedSearchCV\n",
    "\n",
    "random_grid = {'n_estimators': [int(x) for x in np.linspace(10, 50, num = 2)],\n",
    "               'max_features': ['auto','sqrt'],\n",
    "               'max_depth': [int(x) for x in np.linspace(10,50 , num = 2)],\n",
    "               'min_samples_split': [int(x) for x in np.linspace(2, 11, num = 2)],\n",
    "               'min_samples_leaf': [int(x) for x in np.linspace(2, 11, num = 2)],\n",
    "               'bootstrap': [True, False]}\n",
    "\n",
    "rf_random = RandomizedSearchCV(estimator = RandomForestClassifier(), \n",
    "                               param_distributions = random_grid, \n",
    "                               n_iter=50, \n",
    "                               cv=2, \n",
    "                               verbose=2, \n",
    "                               random_state=42, \n",
    "                               scoring='f1_micro',\n",
    "                               n_jobs = -1)\n",
    "\n",
    "rf_random.fit(X_train, y_train)\n",
    "print(rf_random.best_estimator_)\n"
