    "from sklearn.model_selection import GridSearchCV\n",
    "from sklearn.metrics import f1_score\n",
    "from sklearn.metrics import make_scorer\n",
    "\n",
    "\n",
    "param_grid = [\n",
    "  {'alpha': [0.000025],\n",
    "   'hidden_layer_sizes': [ [11,9,7]] , \n",
    "   'solver': [\"adam\"],\n",
